&НаСервере
Функция ТаблицаЗначенийПоУмолчанию()
	
	ЗначенияПоУмолчанию = Новый ТаблицаЗначений;
	ЗначенияПоУмолчанию.Колонки.Добавить("Тип", Новый ОписаниеТипов("ПеречислениеСсылка.ТипЭлементаСтруктуры"));
	ЗначенияПоУмолчанию.Колонки.Добавить("Код", Новый ОписаниеТипов("Строка", , Новый КвалификаторыСтроки(50, ДопустимаяДлина.Переменная)));
	ЗначенияПоУмолчанию.Колонки.Добавить("Наименование", Новый ОписаниеТипов("Строка", , Новый КвалификаторыСтроки(150, ДопустимаяДлина.Переменная)));
	ЗначенияПоУмолчанию.Колонки.Добавить("ПривычнаяСортировка", Новый ОписаниеТипов("Число", Новый КвалификаторыЧисла(10, 0, ДопустимыйЗнак.Неотрицательный)));
	ЗначенияПоУмолчанию.Колонки.Добавить("Родитель", Новый ОписаниеТипов("Строка", , Новый КвалификаторыСтроки(256, ДопустимаяДлина.Переменная)));
	ЗначенияПоУмолчанию.Колонки.Добавить("ПринимаемыйТип",Новый ОписаниеТипов("Строка", , Новый КвалификаторыСтроки(1024, ДопустимаяДлина.Переменная)));
	
	ДобавитьОбщие(ЗначенияПоУмолчанию);
	ДобавитьКонстанты(ЗначенияПоУмолчанию);
	ДобавитьСправочники(ЗначенияПоУмолчанию);
	ДобавитьДокументы(ЗначенияПоУмолчанию);
	ДобавитьЖурналыДокументов(ЗначенияПоУмолчанию);
	ДобавитьПеречисления(ЗначенияПоУмолчанию);
	ДобавитьОтчеты(ЗначенияПоУмолчанию);
	ДобавитьОбработки(ЗначенияПоУмолчанию);
	ДобавитьПланыВидовХарактеристик(ЗначенияПоУмолчанию);
	ДобавитьПланыСчетов(ЗначенияПоУмолчанию);
	ДобавитьПланыВидовРасчета(ЗначенияПоУмолчанию);
	ДобавитьРегистрыСведений(ЗначенияПоУмолчанию);
	ДобавитьРегистрыНакопления(ЗначенияПоУмолчанию);
	ДобавитьРегистрыБухгалтерии(ЗначенияПоУмолчанию);
	ДобавитьРегистрыРасчета(ЗначенияПоУмолчанию);
	ДобавитьБизнесПроцессы(ЗначенияПоУмолчанию);
	ДобавитьЗадачи(ЗначенияПоУмолчанию);
	ДобавитьВнешниеИсточникиДанных(ЗначенияПоУмолчанию);
	
	Возврат ЗначенияПоУмолчанию;
	
КонецФункции

